<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yangfei.singlefunction.mapper.CourseMapper">
    <select id="selectByFirst" resultType="com.yangfei.singlefunction.entity.Course">
        select * from course limit 1
    </select>
    <select id="selectCoursePage" resultType="com.yangfei.singlefunction.entity.Course">
        select * from course where Cname != #{name}
    </select>
</mapper>
